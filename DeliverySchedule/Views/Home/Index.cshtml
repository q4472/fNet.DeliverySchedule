<div id="deliveryschedule_views_home_index">
    @if (HttpContext.Current.IsDebuggingEnabled)
    {
        <div>Debugging is enabled.</div>
    }
    <input type="text" name="код" value="122740" />
    <input type="button" value="Перейти к графику поставок" onclick="DeliveryScheduleViewsHomeIndex.goToDeliverySchedulePage()" />
</div>
<script type="text/javascript">
    var DeliveryScheduleViewsHomeIndex = (function () {
        return {
            goToDeliverySchedulePage: function () {
                var rqp = {
                    SessionId: '00000000-0000-0000-0000-000000000000',
                    Command: 'GoToDeliverySchedule',
                    Parameters: [
                        { Name: 'id', Value: $('#deliveryschedule_views_home_index input[name="код"]').val() }
                    ]
                };
                Nskd.Http.post({
                    url: '/DeliverySchedule/F3',
                    rqp: rqp,
                    done: function (data) {
                        $('#_layout_content').html(data);
                    }
                });
            }
        };
    })();
</script>
